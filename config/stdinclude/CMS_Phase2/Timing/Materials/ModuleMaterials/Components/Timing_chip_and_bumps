// Timing chip and bumps.

Component {
   componentName "Timing chip and Bumps"
   service false
   //Chip on sensors
   Element {
     elementName Si
     quantity 2.069
     unit g
     scaleOnSensor 0
     targetVolume 7
   }
   //Chip periphery
   Element {
     elementName Si
     quantity 0.256
     unit g
     scaleOnSensor 0
     targetVolume 56
   }
   // Underfill
   Element {
     elementName Epoxy
     quantity 0.874
     unit g
     scaleOnSensor 0
     targetVolume 7
   }
   // Bumps
   Element {
     elementName Sn95Ag5
     quantity 0.020  // Guess from the PS. Quantity from PS divided by 20 (numStripsAcross = 960 in PS and reference numStripsAcross = 48).
     unit g
     scaleOnSensor 1  // Quantity gets adjusted in case timing module has different number of channels from reference sensor.
     targetVolume 7
   }
}
