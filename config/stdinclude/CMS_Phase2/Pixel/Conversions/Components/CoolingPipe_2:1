// Cooling pipe : 2 -> 1

  // Titanium pipe
  Conversion {
    Input {
      Element {
        elementName Ti 
        quantity 2
        unit g/m
      }
    }
    Output {
      Element {
        elementName Ti
        quantity 1
        unit g/m
        service true
      }
    }  
  }
